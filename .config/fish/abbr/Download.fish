# aria2
abbr -a ariadl  'aria2c -j 8 -x 8 -s 8 --summary-interval=1 --console-log-level=warn --download-result=hide --continue=true --auto-file-renaming=true'

# wget: progress bar + speed shown by default
abbr -a wdl 'wget --show-progress'

# curl: enable full progress bar + auto filename
abbr -a cdl 'curl -L -O --progress-bar'
